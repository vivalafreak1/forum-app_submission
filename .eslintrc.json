{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "cypress"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-alert": "off",
        "no-underscore-dangle": "off",
        "react/jsx-props-no-spreading": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "react/require-default-props": "off",
        "no-unused-vars": "off"
    }
}
